CC=gcc
SRC=main.c holberton.c
OBJ=$(SRC:%.c=%.o)
NAME=holberton
RM=rm -f
CFLAGS=-Wall -Werror -Wextra -pedantic
.PHONY: re oclean fclean clean all

all : $(NAME)

$(NAME) : m.h $(OBJ)
            $(CC) $(OBJ) -o $@

clean :
		$(RM) *~ $(NAME)

oclean :
		$(RM) $(OBJ)

fclean : clean oclean

re : oclean all
 